{
  "name": "clinic-messenger",
  "version": "1.0.0",
  "description": "ClinicMessenger - Automated WhatsApp messaging system for healthcare clinics",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "concurrently \"electron-vite dev\" \"cd admin && npm run dev\"",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "convert-icon": "node scripts/convert-icon.js",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run convert-icon && npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-router": "^1.139.14",
    "@tanstack/react-router-devtools": "^1.139.15",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mssql": "^9.1.8",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.3.9",
    "express": "^5.2.1",
    "lucide-react": "^0.574.0",
    "mssql": "^12.2.0",
    "node-cron": "^4.2.1",
    "node-schedule": "^2.1.1",
    "puppeteer": "^24.32.0",
    "qrcode.react": "^4.2.0",
    "react-hook-form": "^7.68.0",
    "tailwindcss": "^4.1.17",
    "whatsapp-web.js": "^1.34.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@tanstack/router-plugin": "^1.139.14",
    "@types/node": "^25.2.3",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^40.4.1",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
